<h1>Create a new School</h1>

<%= form_for(@school) do |f| %>
  <ul>
  <% @school.errors.full_messages.each do |error| %>
    <li><%= error %></li>
  <% end %>
  </ul>
  <p>
    <%= f.label 'School Name' %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label 'PO Number' %><br />
    <%= f.text_field :poNumber %>
  </p>
  <h2>School Address</h2>
  <p>
    <%= f.label 'Street' %><br />
    <%= f.text_area :addressStreet %>
  </p>
  <p>
    <%= f.label 'City' %><br />
    <%= f.text_area :addressCity %>
  </p>
  <p>
    <%= f.label 'State' %><br />
    <%= f.text_area :addressState %>
  </p>
  <p>
    <%= f.label 'Zip Code' %><br />
    <%= f.text_area :addressZip %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>